# Ejemplo de archivo .env
# Copia este archivo como .env y actualiza los valores según tu entorno

# Base de datos
DATABASE_URL=postgresql://usuario:password@localhost/nombre_base_datos

# Seguridad
SECRET_KEY=genera_una_clave_secreta_con_openssl_rand_hex_32
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Aplicación
APP_NAME=Solandre API
APP_VERSION=1.0.0
DEBUG=True
ENVIRONMENT=development

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Servidor
HOST=127.0.0.1
PORT=8000

# Delivery
DELIVERY_ASSIGNMENT_TIMEOUT=10
